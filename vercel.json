{
    "version": 2,
    "builds": [
      { "src": "package.json", "use": "@vercel/static-build" }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "build/static/$1" },
      { "src": "/manifest.json", "dest": "build/manifest.json" },
      { "src": "/favicon.ico", "dest": "build/favicon.ico" },
      { "src": "/lessons/(.*)", "dest": "public/lessons/$1" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  